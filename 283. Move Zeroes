class Solution {
    public void moveZeroes(int[] nums) {
    int i=0,temp,len,j;
        
        for(i=0,j=1;j<nums.length;j++)
        {temp=0;
        if(nums[i] == 0 && nums[j] != 0)
        {temp=nums[i];
         nums[i]=nums[j];
         nums[j]=temp;
        }
         if(nums[i]!=0)
             i++;
        }
     }   
    }
